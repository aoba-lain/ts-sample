{
  "name": "ts-sample",
  "version": "1.0.0",
  "description": "sample",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "build": "parcel build --target node src/index.ts",
    "watch": "run-p --print-label watch:*",
    "watch:build": "parcel watch --target node src/index.ts",
    "watch:lint": "esw --watch --color --cache --ext ts .",
    "lint": "eslint --ignore-path .gitignore --ext .ts .",
    "fix": "yarn lint --fix",
    "clean": "rimraf dist/*"
  },
  "repository": "https://github.com/aoba-lain/ts-sample",
  "author": "aoba-lain <aoba.lain@protonmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-typescript": "^6.1.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-tsc": "^1.2.0",
    "eslint-watch": "^6.0.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
